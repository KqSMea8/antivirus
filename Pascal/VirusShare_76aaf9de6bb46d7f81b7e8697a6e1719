    var isPhone = /iPhone|iPod|Phone|Android/i.test(navigator.userAgent);
    var product_zoom = null;
    var urlsite = 'http://www.airsoftlegend.com/';
    var AJAXCART_AUTOCLOSE = 5;
    var FREEZED_TOP_MENU = 0;
